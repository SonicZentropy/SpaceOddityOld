{
	EditorInfosPosition = int2(-508, 22);
	Expression = "function void	Eval()
{
	Life = 1.0;
	
	Position= Shape_0.samplePosition();
	
	float ID = ninja;
	
	Random=ninja;
	
	float NoiseHue = abs(noise(float4(Position*15,scene.Time)));
	
	float NoiseOpacity = pow(abs(noise(float4(Position*10,scene.Time))),1.5);
	
	float4 Hue = float4(NoiseHue,0.5,1,1)*select(0.2,1,ID<0.02);
	
	BaseColor=lerp(Hue,Hue.x001,abs(Position.x)*RedBorder)*NoiseOpacity*20;
	
	SizeCoeff= select(ParticleScale,ParticleScale*2,ID<0.02);
}
";
}
