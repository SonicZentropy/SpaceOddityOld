CCompilerSyntaxNodeExpression	$LOCAL$/CCompilerSyntaxNodeExpression_1488CA33
{
	Expression = "function void	Eval()
{
	float Distance = length(Position-Target);
	
	Fov =max(min(Distance*Focal,1),0);

	Size=SizeCoeff+pow(Fov,8)*Blur*Distance;
	
	float	areaSharp = SizeCoeff*SizeCoeff;
	float	areaBlurred = Size*Size;
//	float	dofFade = 1-pow(Fov,0.5)*(1-min(max(pow(Blur,8),0.01),1));
	float	dofFade = areaSharp / areaBlurred;

	Color=lerp(select(100,1,Random<0.9),1,pow(Fov,0.25))*BaseColor*Curve_0.sample(LifeRatio)*0.5*dofFade;
}
";
}
