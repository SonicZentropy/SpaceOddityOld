	Position = Sphere.samplePosition(pCoords);
	Normal = normalize(Position);
	NormalForAlignment = Normal;
	Tangent = cross(Normal, scene.axisUp());

	float	killer = noise(Position*6);
	Life = select(0, infinity, killer > 0.2);
}
";
}
