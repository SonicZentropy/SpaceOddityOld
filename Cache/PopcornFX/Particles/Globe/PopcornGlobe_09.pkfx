	float3	normal = normalize(Position);
	float	globeSide = -dot(normal, normalize(Position - ViewPosition)) * 0.5 + 0.5;
	float	colorIntensity = pow(noise(float3(Position.xz*5,scene.Time*0.7))*0.5+0.5,2)*2+0.1;
