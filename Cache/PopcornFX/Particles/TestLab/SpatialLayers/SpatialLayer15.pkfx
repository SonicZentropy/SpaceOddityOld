CCompilerSyntaxNodeExpression	$LOCAL$/CCompilerSyntaxNodeExpression_F54E7557
{
	Expression = "function void	Eval()
{
//	float	d = saturate(length(Position - ViewPosition)*0.2);
	float	neighborCount = spatialLayers.DroppedSnow.neighborCount(Position, Size*(0.1/0.08));
	
	float3	d = Position - spatialLayers.DroppedSnow.Position.average(Position, Size*(0.25/0.08));
	float3	na = safe_normalize(d);
	float3	n = select(na, float3(1,0,0), abs(d.x) == infinity);
	Normal = d;
	
//	float3	n = safe_normalize(d);
	float3	light = normalize(float3(2, 1, -1));
	
	float	ss = 1.2 * 0.5;
	float	l = pow(saturate(dot(n,light) * (1-ss) + ss),2);
//	float	l = saturate(neighborCount / 80);
	Color = lerp(float4(0.05,0.07,0.15,1), float4(1,1,1,1), l * pow(saturate((Position.y+5)*1.0),1));
	
//	int		shouldSleep = n.y < -0.5 && neighborCount > 8;
	int	shouldSleep = n.y < -0.1 && length(d) < Size*1.5 && neighborCount > 5;
//	Color = select(Color, float4(1,0,0,1), shouldSleep);
	
	SwitchToStatic.trigger(shouldSleep);
	kill(shouldSleep);
}
";
}
