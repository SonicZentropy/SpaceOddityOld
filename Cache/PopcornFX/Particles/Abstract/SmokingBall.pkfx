CCompilerSyntaxNodeExpression	$LOCAL$/CCompilerSyntaxNodeExpression_FCA284D1
{
	Expression = "function void	Eval()
{
	VelocityField = Turb.sample(Position) * ((1 - N) * 0.6 + 0.4);
	VelocityField += N * Normal * (1 - LifeRatio) * 5;
	
	//float3		vec = Position - Origin;
	Normal = rotate(Normal, Side, -dt * 4 * N);
	
	float		rad = 0.8;
	float3		pos = spatialLayers.SmookBalls.Position.average(Position, rad);
	//float3		pos = spatialLayers.SmookBalls.Position.averageKernel(Position, rad, Ker);
	//float		count = spatialLayers.SmookBalls.neighborCount(Position, rad);
	//float		inFac = exp(-count * 0.005);

	float3		litDir = float3(0, -1, 0);
	float		litFac = dot(-safe_normalize(Position - pos), litDir);
	litFac = pow(litFac * 0.5 + 0.5, 2);
	
	float4		base = SampColor.sample(BurnBase * (1 - LifeRatio));
	float3		color = base.xyz * (litFac + pow(BurnBase, 8)*10);
	
	Color = float4(iif(pos.x != infinity, color, 1) * 0.3 + 0.02, 1);
	Color *= Alpha.sample(LifeRatio);
	Color = float4(Color.xyz, Color.w * base.w);
}
";
}
