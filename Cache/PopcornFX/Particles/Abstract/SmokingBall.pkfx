	EditorInfosPosition = int2(-508, 22);
	Expression = "function void	Eval()
{
	float	n =  noise(scene.Time * 3) * 0.5 + 0.5;
	n = pow(n, 4) * 0.9 + 0.1;
	//n = 1;

	Mass = 1;
	Life = pow(ninja, 2) * 1 + 2;
	Size = 0.08;
	
	int3		pcoord = Ball.sampleParametricCoords();

	Position = Ball.samplePosition(pcoord);
	Normal = Ball.sampleNormal(pcoord);
	N = n;
}

function void		PostEval()
{
	Side = safe_normalize(cross(Normal, Velocity), float3(0, 0, 1));
	Velocity = Velocity * N * 1.3 + Normal * N * 10;
	
	float		burn = fast_noise(Position * 5) * 0.5 + 0.5;
	burn = pow(burn, 0.8);
	BurnBase = burn;//SampColor.sample(burn);
	BurnScale = rcp(1 - BurnBase);
}
";
}
