CCompilerSyntaxNodeExpression	$LOCAL$/CCompilerSyntaxNodeExpression_738A65A9
{
	Expression = "function void	Eval()
{
	int2	blockAndDigit = BlockAndDigit;
	
	// extract value from the 8 values stored in the two float4 attributes:
	int4	blockId = blockAndDigit.x;
	int4	batch0 = ValueBatch0 & (blockId == int4(0,1,2,3));
	int4	batch1 = ValueBatch1 & (blockId == int4(4,5,6,7));
	int4	batch2 = batch0 + batch1;
	int		value = batch2.x + batch2.y + batch2.z + batch2.w;
	
	// extract digit in the [0, 15] range
	int		digit = (value >> blockAndDigit.y) % 16;
	
	// if digit is in the [0,9] range, have TextureID start at 16 (\'0\' in our atlas)
	// if digit is in the [10,15] range, have TextureID start at 33 (\'A\' in our atlas), for hexadecimal characters
	TextureID = digit + select(16, 33 - 10, digit >= 10);
}
";
}
