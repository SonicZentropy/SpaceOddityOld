CCompilerSyntaxNodeExpression	$LOCAL$/CCompilerSyntaxNodeExpression_871FBCEC
{
	EditorInfosPosition = int2(-508, 22);
	Expression = "function void	Eval()
{
	float	spawnCount = 6400;		// same value as in the \'SpawnCount\' property of the layer
	float	particlesPerDepth = 64;
	float	particlesPerBlock = 8;	// we\'re displaying 8-digit numbers
	float	layerCount = spawnCount / particlesPerDepth;
	
	float	rawId = floor(spawner.LifeRatio * spawnCount);
	float	layer = floor(rawId / particlesPerDepth);
	float	particleId = rawId - layer * particlesPerDepth;
	float	block = floor(particleId / particlesPerBlock);
	float	digitId = particleId - block * particlesPerBlock;

	// note: here, for optimization, instead of storing the raw digitId in \'y\',
	// we\'ll store 4 * (7 - digitId) so that we can use that as a bit-shift count at evolve
	BlockAndDigit = int2(int(block), 4 * (7 - int(digitId)));
	
	Life = infinity;
	Size = 0.1;
	TextureID = 0;
	
	AxisUp = scene.axisUp();
	AxisNormal = scene.axisForward();
	
	float	atlasRectAspect = 0.5;	// the texture rects witchs are half the heights. (Matches the \'AxisScale\' property of the planar billboarder)
	float	xOffset = -8 - 1;
	Position = float3((digitId + xOffset) * Size * atlasRectAspect, block * Size, layer*-0.02);
	
	float	a = (pow(1 - saturate(layer / layerCount),4) * 0.998 + 0.002) * 0.1;
	Color = select(float4(1,1,1,a), float4(1,5,1,1), layer == 0);
}
";
}
